{
  "name": "frode-web",
  "version": "1.0.0",
  "description": "Frode Bjerkes personal website",
  "main": "index.js",
  "scripts": {
    "dev": "concurrent 'nodemon .' 'npm run watch'",
    "build": "npm run build:css && npm run build:js",
    "build:css": "lessc frontend/less/main.less --autoprefix=\"last 2 versions\" dist/styles.css",
    "build:js": "browserify frontend/js/main.js -t babelify -o dist/scripts.js",
    "watch": "npm run watch:css & npm run watch:js",
    "watch:css": "watch 'npm run build:css' frontend/less",
    "watch:js": "watchify -v -t babelify frontend/js/main.js -o dist/scripts.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "frodebjerke",
  "license": "ISC",
  "dependencies": {
    "babel": "6.x",
    "babel-core": "6.x",
    "babel-plugin-transform-object-rest-spread": "6.x",
    "babel-polyfill": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "express": "4.x",
    "history": "^2.0.0-rc2",
    "morgan": "1.x",
    "react": "latest",
    "react-dom": "latest",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "redux": "^3.0.5",
    "redux-logger": "^2.4.0",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^1.0.3",
    "winston": "2.x"
  },
  "devDependencies": {
    "nodemon": "latest",
    "concurrently": "latest",
    "browserify": "latest",
    "babelify": "latest",
    "less": "latest",
    "less-plugin-autoprefix": "latest",
    "watch": "latest",
    "watchify": "latest"
  }
}
